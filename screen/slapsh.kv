MDScreen:
    name:"splash"
    MDFloatLayout:
        md_bg_color: 9/255, 10/255, 24/255, 0.8
        Image:
            source:"data/logo/logo.png"
            size_hint: .26, .26
            pos_hint: {"center_x": .5, "center_y": .52}
            canvas.before:
                Color:
                    rgb: 1, 1, 1, 1
                Ellipse:
                    size: 130, 130 
                    pos: 110, 250

    MDLabel:
        id: medic
        text: "Medic Piel"
        pos_hint: {"center_x": .5, "center_y": .2}
        halign: "center"
        theme_text_color: "Custom"
        text_color: 1, 1, 1, 1
        font_size: "35sp"
        font_name: "data/font/Poppins-Semibold.ttf"  

    MDLabel:
        id: innova
        text: "Innovacion y tecnologia"
        pos_hint: {"center_x": .5, "center_y": .15}
        halign: "center"
        theme_text_color: "Custom"
        text_color: 1, 1, 1, 1
        font_size: "15sp"
        font_name: "data/font/Poppins-Regular.ttf"